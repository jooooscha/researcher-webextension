{
  "name": "researcher-webextension",
  "displayName": "RE:SEARCHER",
  "description": "RE:SEARCHER is a personal search engine for your bookmarks.",
  "version": "0.10.0",
  "author": "andots",
  "license": "MIT",
  "homepage": "https://github.com/andots/researcher-webextension",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:andots/researcher-webextension.git"
  },
  "bugs": {
    "url": "https://github.com/andots/researcher-webextension/issues"
  },
  "keywords": [
    "search",
    "bookmarks",
    "fulltext-search",
    "elasticsearch",
    "react",
    "preact",
    "webextension"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:prepare build:web build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build --config vite.config.ts",
    "build:js": "vite build --config vite.config.content.ts",
    "zip": "run-s zip:webext zip:archive",
    "zip:webext": "web-ext build --artifacts-dir=./build --source-dir=./extension --overwrite-dest --no-config-discovery",
    "zip:archive": "git archive --format=zip --output=./build/source.zip HEAD",
    "test": "node --experimental-vm-modules jest.js",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "lint": "eslint src/**/*.{json,ts,tsx,js}",
    "release": "run-s build zip"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@preact/preset-vite": "^2.10.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/preact": "^3.2.4",
    "@types/dompurify": "^3.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@types/redux-persist-webextension-storage": "^1.0.3",
    "@types/webextension-polyfill": "^0.12.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-config-preact": "^1.5.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jest-webextension-mock": "^4.0.0",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.26.5",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redux": "^5.0.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.87.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "web-ext": "^8.6.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mozilla/readability": "^0.6.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/lab": "7.0.0-beta.11",
    "@mui/material": "^7.0.2",
    "@reduxjs/toolkit": "^2.7.0",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "dompurify": "^3.2.5",
    "escape-goat": "^4.0.0",
    "highlight.js": "^11.11.1",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.0.5",
    "interweave": "^13.1.1",
    "p-limit": "^6.2.0",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "react-use": "^17.6.0",
    "react-window": "^1.8.11",
    "redux-persist": "^6.0.0",
    "redux-persist-webextension-storage": "^1.0.2",
    "serialize-error": "^12.0.0",
    "tree-model": "^1.0.7"
  }
}
